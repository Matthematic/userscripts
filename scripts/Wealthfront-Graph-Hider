// ==UserScript==
// @name         Wealthfront Graph Hider
// @namespace    https://github.com/Matthematic/userscripts
// @version      2024-07-01
// @description  Removes the graph from the Wealthfront dashboard
// @author       Matthematic
// @match        https://www.wealthfront.com/dashboard
// @icon         https://www.google.com/s2/favicons?sz=64&domain=wealthfront.com
// @grant        none
// ==/UserScript==

(function() {
    'use strict';

    const css = `
        .dashboard-components-dashboard-overview-overview-graph {
            display: none!important;
        }
    `;

    function injectCSS() {
        const style = document.createElement('style');
        style.type = 'text/css';
        style.appendChild(document.createTextNode(css));
        document.head.appendChild(style);
    }

    injectCSS();
})();
